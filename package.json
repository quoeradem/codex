{
  "name": "codex",
  "version": "0.2.0-alpha",
  "description": "A content-driven framework designed to beautifully present a collection of markdown documents",
  "main": "index.js",
  "scripts": {
    "prestart:dev": "npm run build:dev",
    "build:dev": "webpack --config webpack.config.dev.js",
    "start:dev": "NODE_ENV=development nodemon index.js --ignore shared --exec babel-node",
    "prestart": "rm -rf build && npm run build:server && npm run build",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js -p",
    "build:server": "NODE_ENV=production webpack --config webpack.config.server.js -p",
    "start": "NODE_ENV=production node build/server.js"
  },
  "author": "Trevor Kulhanek",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "chokidar": "^1.6.1",
    "github-slugger": "^1.1.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.0.0-alpha.7",
    "koa-bodyparser": "^4.2.0",
    "koa-compress": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "material-ui": "^0.18.1",
    "mdast-util-to-string": "^1.0.2",
    "mdast-util-toc": "^2.0.1",
    "prismjs": "^1.6.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "remark": "^7.0.1",
    "remark-react": "^4.0.0",
    "remark-slug": "^4.2.2",
    "webpack": "^2.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-es2015-destructuring": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.11.1",
    "koa-webpack-dev-middleware": "^1.3.0",
    "koa-webpack-hot-middleware": "^1.0.3",
    "node-sass": "^4.2.0",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.7"
  }
}
